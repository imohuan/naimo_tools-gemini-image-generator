{
  "name": "naimo-plugin-template",
  "version": "1.0.0",
  "description": "Naimo Tools 插件开发模板",
  "type": "module",
  "scripts": {
    "init": "pnpm i && pnpm run add-electron-types && pnpm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "add-electron-types": "$env:ELECTRON_SKIP_DOWNLOAD=1; pnpm install electron --save-dev",
    "deploy": "node scripts/deploy-to-build.js"
  },
  "keywords": [
    "naimo",
    "plugin",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "jszip": "^3.10.1",
    "pinia": "^3.0.4",
    "vue": "^3.5.25"
  }
}
